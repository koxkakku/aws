yum update -y
yum install httpd
systemctl start httpd
systemctl enable httpd
echo "Hello EC2" > var/www/html/index.html
systemctl status httpd
curl http://169.254.169.254/latest/meta-data
systemctl status httpd
curl http://169.254.169.254/latest/meta-data
curl http://169.254.169.254/latest/meta-data
exit

